<h1>Crear</h1>
<%= form_for @longTask, url: {:action => :create } do |f| %>
  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :state %><br>
    <%= f.select :state, ['Pediente','curso', 'Hecha'] %><br>
    
  </p>
  <p>
    <%= f.label :priority %><br>
    <%= f.select :priority, ['alta', 'media','baja'] %>
  </p>
  <p>
    <%= f.label :porcentaje_de_avance %><br>
    <%= f.select :percentage, (0..100) %>%
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>