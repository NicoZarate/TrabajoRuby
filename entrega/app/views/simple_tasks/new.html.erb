<h1>Create </h1>
<%= form_for @simpleTask, url: { :action => :create } do |f| %>
  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :state %><br>
    <%= f.select :state, ['Pediente', 'Hecha'] %><br>
    
  </p>
  <p>
    <%= f.label :priority %><br>
    <%= f.select :priority, ['alta', 'media','baja'] %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>