<%= link_to 'New List', new_list_path %>
<h1>Listing lists</h1>
 
<table>
  <tr>
    <th>nombre</th>
    <th>Url</th>
  </tr>
 
  <% @lists.each do |list| %>
    <tr>
      <td><%= list.name %></td>
      <td><%= list.url %></td>
      <td><%= link_to 'Show', list_path(list) %></td>
      <td><%= link_to 'Edit', edit_list_path(list) %></td>
    </tr>
  <% end %>
</table>